{
  "cli": {
    "version": ">= 12.5.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BACKEND_BASE_URI": "http://bastiat.hopto.org:3020"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_BASE_URI": "http://bastiat.hopto.org:3020"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
